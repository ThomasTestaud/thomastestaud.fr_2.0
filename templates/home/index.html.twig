{% extends 'base.html.twig' %}

{% block title %}
	Home |
	{{ parent() }}
{% endblock %}

{% block stylesheets %}
	{{ parent() }}
	<link rel="stylesheet" href="{{ asset('style/home.css') }}">
{% endblock %}

{% block script %}
	{{ parent() }}
	<script src="{{ asset('js/home.js') }}"></script>
{% endblock %}

{% block nav %}
	<a href="/CV">
		<p>Consulter le CV</p>
	</a>
	<a href="Portfolio">
		<p>Consulter le PortFolio</p>
	</a>
{% endblock %}

{% block body %}

	<div class="main">
		<div>
			<h1>{{profile.name}}</h1>
			<p>{{profile.title}}</p>
		</div>
		<img src="images/{{profile.photo}}" alt="Photo de {{profile.name}}">
	</div>
{% endblock %}
